{
  "name": "jhipster-sample-application",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Sample Application",
  "license": "UNLICENSED",
  "jestSonar": {
    "reportPath": "target/test-results",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path=target/classes/static",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --coverage",
    "lint": "ng lint",
    "prepare": "husky",
    "prettier:check": "prettier --check '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'",
    "prettier:format": "prettier --write '{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml,feature}'"
  },
  "devDependencies": {
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "eslint": "8.57.0",
    "@angular-builders/jest": "17.0.2",
    "@angular-devkit/build-angular": "17.3.1",
    "@angular/cli": "17.3.1",
    "@angular/compiler-cli": "17.3.1",
    "@types/node": "20.11.30",
    "@types/jest": "29.5.12",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.1.2",
    "jest-preset-angular": "14.0.3",
    "jest-sonar-reporter": "2.0.0",
    "typescript": "5.4.3",
    "@prettier/plugin-xml": "3.3.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "prettier-plugin-gherkin": "2.2.1",
    "prettier-plugin-java": "2.6.0",
    "prettier-plugin-packagejson": "2.5.3"
  },
  "dependencies": {
    "@angular/animations": "17.3.1",
    "@angular/cdk": "17.3.1",
    "@angular/common": "17.3.1",
    "@angular/compiler": "17.3.1",
    "@angular/core": "17.3.1",
    "@angular/material": "17.3.1",
    "@angular/forms": "17.3.1",
    "@angular/platform-browser": "17.3.1",
    "@angular/platform-browser-dynamic": "17.3.1",
    "@angular/router": "17.3.1",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.4"
  }
}
